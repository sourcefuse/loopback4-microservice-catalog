		<script name="events_attendees_view" id="Editor_55" language="SQL" >
			<string><![CDATA[CREATE VIEW scheduler.events_attendees_view AS  SELECT et.id,
    et.bg_color,
    et.calendar_id,
    et.created_by,
    et.created_on,
    et.deleted,
    et.description,
    et.end_datetime,
    et.fg_color,
    et.icaluid,
    et.is_full_day_event,
    et.is_locked,
    et.link,
    et.location,
    et.meeting_link,
    et.modified_by,
    et.modified_on,
    et.identifier,
    et.parent_event_id,
    et.start_datetime,
    et.status,
    et.summary,
    et.timezone,
    et.ext_id,
    et.ext_metadata,
    at.id AS attendee_id,
    at.identifier AS attendee_identifier,
    at.event_id,
    at.is_optional,
    at.is_organizer,
    at.messages,
    at.response_status
   FROM (scheduler.events et
     LEFT JOIN scheduler.attendees at ON ((et.id = at.event_id)));]]></string>
