    et.summary,
    et.timezone,
    et.ext_id,
    et.ext_metadata,
    at.id AS attendee_id,
    at.identifier AS attendee_identifier,
    at.event_id,
    at.is_optional,
    at.is_organizer,
    at.messages,
    at.response_status
   FROM (scheduler.events et
     LEFT JOIN scheduler.attendees at ON ((et.id = at.event_id)));]]></view_script>
