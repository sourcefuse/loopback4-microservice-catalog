{
  "name": "@sourceloop/notification-service",
  "version": "1.0.0-alpha.20",
  "description": "services",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "echo \"No tests !\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
    "directory": "services/notification-service"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.1",
    "@loopback/context": "^3.8.2",
    "@loopback/core": "^2.7.0",
    "@loopback/repository": "^2.5.1",
    "@loopback/rest": "^5.0.1",
    "@sourceloop/core": "^1.0.0-alpha.15",
    "aws-sdk": "^2.694.0",
    "lodash": "^4.17.15",
    "loopback4-authentication": "^3.0.1",
    "loopback4-authorization": "3.1.1",
    "loopback4-notifications": "^1.0.0",
    "pubnub": "^4.28.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.1",
    "@loopback/eslint-config": "^7.0.1",
    "@loopback/testlab": "^3.1.5",
    "@types/lodash": "^4.14.153",
    "@types/node": "^10.17.24",
    "eslint": "^7.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
