{
  "name": "@sourceloop/authentication-service",
  "version": "2.0.0",
  "description": "services",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "lb-tsc && npm run openapi-spec && npm run apidocs",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "openapi-spec": "node ./dist/openapi-spec",
    "apidocs": "./node_modules/.bin/widdershins --search false --language_tabs 'javascript:JavaScript:request' 'javascript--nodejs:Node.JS' --summary openapi.json -o openapi.md",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "coverage": "nyc npm run test",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run test",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "postinstall": "node migration.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
    "directory": "services/authentication-service"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "migration.js",
    "migrations",
    "database.json",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "db-migrate": "< 1.0.0 || ^1.0.0",
    "db-migrate-pg": "^1.0.0"
  },
  "dependencies": {
    "@loopback/boot": "^3.4.4",
    "@loopback/context": "^3.18.0",
    "@loopback/core": "^2.18.0",
    "@loopback/openapi-v3": "^6.0.2",
    "@loopback/repository": "^3.7.3",
    "@loopback/rest": "^10.1.0",
    "@loopback/rest-explorer": "^3.3.4",
    "@loopback/service-proxy": "^3.2.4",
    "@sourceloop/core": "^4.0.0",
    "base-64": "^1.0.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^10.0.0",
    "dotenv-extended": "^2.9.0",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback4-authentication": "^5.0.2",
    "loopback4-authorization": "^4.0.0",
    "loopback4-soft-delete": "^4.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "node-fetch": "^3.1.0",
    "passport-apple": "^2.0.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-instagram": "^1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.2",
    "@loopback/cli": "^2.22.1",
    "@loopback/eslint-config": "^11.0.2",
    "@loopback/testlab": "^3.4.4",
    "@types/base-64": "^0.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/i18n": "^0.13.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.178",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.11.12",
    "@types/node-fetch": "^3.0.3",
    "@types/passport-apple": "^1.1.0",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-instagram": "^1.0.0",
    "@types/sinon": "^10.0.6",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.32.0",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "source-map-support": "^0.5.20",
    "typescript": "~4.4.4",
    "widdershins": "^4.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
