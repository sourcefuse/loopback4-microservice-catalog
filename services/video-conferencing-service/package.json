{
  "name": "@sourceloop/video-conferencing-service",
  "version": "3.1.3",
  "description": "Microservice providing Video-Conferencing service",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "lb-tsc && npm run openapi-spec && npm run apidocs",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "openapi-spec": "node ./dist/openapi-spec",
    "apidocs": "./node_modules/.bin/widdershins --search false --language_tabs 'javascript:JavaScript:request' 'javascript--nodejs:Node.JS' --summary openapi.json -o openapi.md",
    "pretest": "npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-nyc lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "prepublishOnly": "npm run test",
    "postinstall": "node migration.js && patch-package"
  },
  "repository": {
    "type": "git"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "migration.js",
    "migrations",
    "database.json",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "patch-package": "^6.4.7"
  },
  "dependencies": {
    "@loopback/boot": "^4.1.2",
    "@loopback/context": "^4.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/metadata": "^4.1.2",
    "@loopback/openapi-v3": "^7.1.2",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/service-proxy": "^4.1.2",
    "@sourceloop/core": "^6.1.3",
    "@types/proxyquire": "^1.3.28",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "dotenv-extended": "^2.9.0",
    "loopback-connector-postgresql": "^5.5.0",
    "loopback4-authentication": "^6.0.3",
    "loopback4-authorization": "^5.0.3",
    "loopback4-soft-delete": "^5.0.4",
    "moment": "^2.29.3",
    "nanoid": "^3.3.3",
    "opentok": "^2.14.1",
    "proxyquire": "^2.1.3",
    "tslib": "^2.3.1",
    "twilio": "^3.76.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/testlab": "^4.1.2",
    "@types/axios": "^0.14.0",
    "@types/crypto-random-string": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.25",
    "@types/opentok": "^2.13.1",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "axios-mock-adapter": "^1.20.0",
    "db-migrate": "0.11.13",
    "db-migrate-pg": "1.2.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-mocha": "^10.0.4",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "sinon": "^13.0.2",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.3",
    "widdershins": "^4.0.1"
  },
  "overrides":{
    "jake": {
      "async": "^3.2.3"
    },
    "passport-azure-ad": {
      "async": "^3.2.3"
    },
    "cache-manager": {
      "async": "^3.2.3"
    },
    "winston": {
      "async": "^3.2.3"
    },
    "widdershins": {
      "markdown-it": "^12.3.2"
    },
    "yargs": {
      "yargs-parser": "^13.1.2"
    },
    "better-ajv-errors": {
      "jsonpointer": "^5.0.0"
    },
    "oas-validator": {
      "ajv": "^6.12.6"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
