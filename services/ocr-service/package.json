{
  "name": "@sourceloop/ocr-service",
  "version": "0.2.2",
  "description": "services",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:fix": "npm run prettier:cli -- --write",
    "prettier:check": "npm run prettier:cli",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "pretest": "npm run build",
    "test": "echo \"No tests !\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
    "directory": "services/ocr-service"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "private": true,
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.0",
    "@loopback/context": "^5.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/openapi-v3": "^8.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/rest-explorer": "^5.0.0",
    "@loopback/service-proxy": "^5.0.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/plugin-dns": "^0.15.0",
    "@opentelemetry/plugin-http": "^0.18.2",
    "@opentelemetry/plugin-https": "^0.18.2",
    "@opentelemetry/plugin-pg": "^0.15.0",
    "@opentelemetry/plugin-pg-pool": "^0.15.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@sourceloop/core": "^7.0.2",
    "loopback-connector-mysql": "^6.1.0",
    "loopback-connector-postgresql": "^5.5.0",
    "loopback4-authentication": "^6.1.2",
    "loopback4-authorization": "^5.0.5",
    "loopback4-s3": "^5.1.2",
    "node-fetch": "^3.2.4",
    "swagger-stats": "^0.99.2",
    "symlink-resolver": "",
    "tslib": "^2.4.0",
    "url-join": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@loopback/build": "^9.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/node": "^12.20.50",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "dotenv-extended": "^2.9.0",
    "eslint": "^8.14.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.4",
    "widdershins": "^4.0.1"
  },
  "overrides": {
    "widdershins": {
      "swagger2openapi": "^7.0.8",
      "markdown-it": "^12.3.2",
      "yargs": "^17.4.1"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
