{
  "name": "bpmn-service",
  "version": "1.0.0-alpha.0",
  "description": "bpmn-service",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-nyc lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-nyc lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "Sourcefuse",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/core": "^2.12.0",
    "@loopback/openapi-v3": "^4.0.1",
    "@loopback/repository": "^3.2.0",
    "@loopback/rest": "^9.0.0",
    "@sourceloop/core": "^1.0.0-alpha.27",
    "ajv": "^6.12.6",
    "loopback-connector-postgresql": "^5.3.0",
    "loopback4-authentication": "^4.1.0",
    "loopback4-authorization": "^3.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.6",
    "@loopback/eslint-config": "^10.0.2",
    "@loopback/testlab": "^3.2.8",
    "@types/node": "^10.17.44",
    "eslint": "^7.12.1",
    "source-map-support": "^0.5.19",
    "typescript": "^4.0.5"
  }
}
