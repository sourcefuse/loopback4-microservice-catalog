{
  "name": "@sourceloop/cache",
  "version": "0.3.2",
  "description": "This package provides a mixin which works with redis to cache GET request responses",
  "keywords": [
    "cache",
    "redis",
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\" --timeout 10000",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "coverage": "lb-nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/core": "^4.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "loopback-connector-kv-redis": "^4.0.0"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.0",
    "@loopback/core": "^4.0.0",
    "@loopback/eslint-config": "^13.0.0",
    "@loopback/repository": "^5.0.0",
    "@loopback/rest": "^12.0.0",
    "@loopback/testlab": "^5.0.0",
    "@types/node": "^14.18.21",
    "dotenv": "^16.0.1",
    "dotenv-extended": "^2.9.0",
    "eslint": "^8.17.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
