{
    "name": "@sourceloop/cache",
    "version": "0.6.6",
    "description": "This package provides a mixin which works with redis to cache GET request responses",
    "keywords": [
        "cache",
        "redis",
        "loopback-extension",
        "loopback"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": "16 || 17 || 18"
    },
    "scripts": {
        "build": "lb-tsc",
        "build:watch": "lb-tsc --watch",
        "lint": "npm run eslint && npm run prettier:check",
        "lint:fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
        "prettier:check": "npm run prettier:cli -- -l",
        "prettier:fix": "npm run prettier:cli -- --write",
        "eslint": "eslint --report-unused-disable-directives .",
        "eslint:fix": "npm run eslint -- --fix",
        "pretest": "npm run rebuild",
        "test": "lb-mocha --allow-console-logs \"dist/__tests__\" --timeout 10000",
        "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
        "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
        "rebuild": "npm run clean && npm run build",
        "coverage": "lb-nyc npm run test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git"
    },
    "author": "Sourcefuse",
    "license": "MIT",
    "files": [
        "README.md",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "peerDependencies": {
        "@loopback/core": "^5.1.3",
        "@loopback/repository": "^6.1.3",
        "@loopback/rest": "^13.1.0",
        "loopback-connector-kv-redis": "^4.0.0"
    },
    "dependencies": {
        "@loopback/sequelize": "^0.5.2",
        "tslib": "^2.5.3"
    },
    "devDependencies": {
        "@loopback/build": "^10.1.3",
        "@loopback/core": "^5.1.3",
        "@loopback/eslint-config": "^14.0.4",
        "@loopback/repository": "^6.1.3",
        "@loopback/rest": "^13.1.0",
        "@loopback/testlab": "^6.1.3",
        "@sourceloop/core": "^8.2.0",
        "@types/node": "^18.11.18",
        "dotenv": "^16.0.3",
        "dotenv-extended": "^2.9.0",
        "eslint": "^8.44.0",
        "source-map-support": "^0.5.21",
        "typescript": "~4.9.5"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "typedoc": {
        "config": {
            "entryPoints": [
                "src/index.ts"
            ],
            "out": "packages/cache",
            "plugin": [
                "typedoc-plugin-markdown"
            ]
        }
    }
}