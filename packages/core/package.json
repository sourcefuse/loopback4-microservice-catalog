{
  "name": "@sourceloop/core",
  "version": "2.0.1",
  "description": "packages",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run clean && lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "echo \"No tests !\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.4.0",
    "@loopback/context": "^3.16.0",
    "@loopback/core": "^2.16.0",
    "@loopback/openapi-v3": "^5.3.0",
    "@loopback/repository": "^3.6.0",
    "@loopback/rest": "^9.3.0",
    "@loopback/service-proxy": "^3.2.0",
    "casbin": "^5.2.1",
    "i18n": "^0.10.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "logform": "^2.1.2",
    "loopback-datasource-juggler": "^4.21.2",
    "loopback4-authentication": "^4.6.0",
    "loopback4-authorization": "^3.3.0",
    "loopback4-helmet": "^2.1.0",
    "loopback4-ratelimiter": "^2.2.0",
    "loopback4-soft-delete": "^3.2.2",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "swagger-stats": "^0.95.18",
    "tslib": "^2.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.0",
    "@loopback/eslint-config": "^10.2.0",
    "@loopback/testlab": "^3.4.0",
    "@types/i18n": "^0.8.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.153",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^10.17.59",
    "@types/swagger-stats": "^0.95.3",
    "eslint": "^7.25.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
