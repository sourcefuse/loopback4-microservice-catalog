{
  "name": "@sourceloop/user-onboarding-element",
  "version": "4.0.2",
  "description": "Library for providing a smooth user onboarding.",
  "keywords": [
    "angular-library"
  ],
  "scripts": {
    "checkIfNodeModulesExist": "[ -d \"../../node_modules\" ]",
    "installNodeModules": "cd ../../ && npm i",
    "prebuild": "npm run checkIfNodeModulesExist || npm run installNodeModules",
    "libraryBuild": "ng build userOnboardingLib ",
    "elementBuild": "ng build user-onboarding-element",
    "postbuild": "ts-node bundle-element.ts",
    "build": "npm run libraryBuild && npm run elementBuild",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "echo \"No tests available !\""
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.4",
    "@types/node": "^20.4.1"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public-api.ts"
    },
    "dest": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcefuse/loopback4-microservice-catalog.git",
    "directory": "packages/user-onboarding"
  },
  "author": "Sourcefuse",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "directory": "dist"
  }
}