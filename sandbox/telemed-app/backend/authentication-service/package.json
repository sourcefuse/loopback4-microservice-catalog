{
  "name": "@sourceloop/telemed-auth-api",
  "private": "true",
  "version": "0.0.1",
  "description": "Sourceloop auth service deployed on AWS lambda",
  "keywords": [
    "loopback-microservice",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build:layers": "scripts/build-dependency-layer.sh",
    "build:migrations": "scripts/build-migrations.sh",
    "build": "lb-tsc",
    "build:all": "npm run build && npm run build:layers && npm run build:migrations",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --timeout 1500000 --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "test:lambda": "npm run pretest && lb-mocha --timeout 1500000 --allow-console-logs dist/__tests__/lambda/**/**.js",
    "docker:build": "DOCKER_BUILDKIT=1 sudo docker build --build-arg NR_ENABLED=$NR_ENABLED_VALUE -t $IMAGE_REPO_NAME/authenticationservice-$npm_package_name:$npm_package_version .",
    "docker:run": "docker run -p 3000:3000 -d authenticationservice",
    "symlink-resolver": "symlink-resolver",
    "resolve-links": "npm run symlink-resolver build ./node_modules/@local",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild && npm run openapi-spec",
    "start": "node -r source-map-support/register .",
    "dev": "nodemon --watch src -e ts --exec \"npm run start\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "docker:push": "sudo docker push $IMAGE_REPO_NAME/authenticationservice-$npm_package_name:$npm_package_version",
    "docker:build:dev": "DOCKER_BUILDKIT=1 sudo docker build --build-arg NR_ENABLED=$NR_ENABLED_VALUE -t $IMAGE_REPO_NAME/authenticationservice-$npm_package_name:$IMAGE_TAG_VERSION .",
    "docker:push:dev": "sudo docker push $IMAGE_REPO_NAME/authenticationservice-$npm_package_name:$IMAGE_TAG_VERSION",
    "coverage": "nyc npm run test",
    "db:migrate": "../../node_modules/db-migrate/bin/db-migrate up --config './migration/migrations/database.json'",
    "db:migrate:down": "./node_modules/db-migrate/bin/db-migrate down --config './migrations/database.json'",
    "db:migrate:reset": "./node_modules/db-migrate/bin/db-migrate reset --config './migrations/database.json'"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Vishwas Yogi <vishwas.yogi@sourcefuse.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.379.1",
    "@loopback/boot": "^5.0.1",
    "@loopback/context": "^5.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/openapi-v3": "^8.0.1",
    "@loopback/repository": "^5.0.1",
    "@loopback/rest": "^12.0.1",
    "@loopback/rest-explorer": "^5.0.1",
    "@loopback/service-proxy": "^5.0.1",
    "@sourceloop/audit-log": "^3.2.2",
    "@sourceloop/authentication-service": "10.0.0",
    "@sourceloop/core": "^7.0.1",
    "@types/aws-lambda": "^8.10.110",
    "@vendia/serverless-express": "^4.10.1",
    "dotenv": "^16.0.0",
    "dotenv-extended": "^2.9.0",
    "i18n": "^0.15.1",
    "loopback-connector-postgresql": "^5.5.1",
    "loopback4-authentication": "7.6.0",
    "loopback4-authorization": "^5.0.5",
    "symlink-resolver": "0.2.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@loopback/build": "^9.0.1",
    "@loopback/eslint-config": "^13.0.1",
    "@loopback/testlab": "^5.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/i18n": "^0.13.6",
    "@types/node": "^14.18.21",
    "@types/passport-google-oauth20": "^2.0.11",
    "db-migrate": "^1.0.0-beta.18",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.3"
  }
}
