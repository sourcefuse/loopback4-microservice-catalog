{
  "name": "socketio-service",
  "version": "1.1.0-next.17",
  "description": "A microservice to enable socket system",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "echo \"No tests added !\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "DOCKER_BUILDKIT=1 sudo docker build -t $IMAGE_REPO_NAME/pms-$npm_package_name:$npm_package_version .",
    "docker:push": "sudo docker push $IMAGE_REPO_NAME/pms-$npm_package_name:$npm_package_version",
    "update-deployment-repo": "sed -i 's@IMAGE_REPO_NAME@'$IMAGE_REPO_NAME'@g'",
    "update-deployment-tag-version": "sed -i 's@IMAGE_TAG_VERSION@'$npm_package_version'@g'",
    "docker:build:dev": "DOCKER_BUILDKIT=1 sudo docker build --build-arg NR_ENABLED=$NR_ENABLED_VALUE -t $IMAGE_REPO_NAME/pms-$npm_package_name:$IMAGE_TAG_VERSION .",
    "docker:push:dev": "sudo docker push $IMAGE_REPO_NAME/pms-$npm_package_name:$IMAGE_TAG_VERSION",
    "update-deployment-tag-version:dev": "sed -i 's@IMAGE_TAG_VERSION@'$IMAGE_TAG_VERSION'@g'",
    "deploy:dev": "kubectl apply -f k8s/dev/deployment.yaml -n $NAMESPACE",
    "deploy:qa": "kubectl apply -f k8s/qa/deployment.yaml -n $NAMESPACE",
    "deploy": "kubectl apply -f k8s/deployment.yaml -n $NAMESPACE",
    "docker:run": "docker run -p 3000:3000 -d pms-$npm_package_name",
    "migrate": "node ./dist/migrate",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.4.0",
    "@loopback/context": "^3.16.0",
    "@loopback/core": "^2.16.0",
    "@loopback/express": "^3.3.2",
    "@loopback/http-server": "^2.5.2",
    "@loopback/rest": "^9.3.0",
    "@loopback/rest-explorer": "^3.0.5",
    "@loopback/socketio": "^0.3.2",
    "@sourceloop/core": "^2.0.1",
    "@types/socket.io": "^2.1.11",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.8.0",
    "lodash": "^4.17.21",
    "socket.io": "^2.3.0",
    "socket.io-prometheus-metrics": "^1.0.6",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.6",
    "@loopback/eslint-config": "^10.0.2",
    "@loopback/testlab": "^3.2.8",
    "@types/node": "^10.17.44",
    "eslint": "^7.12.1",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.5"
  }
}
