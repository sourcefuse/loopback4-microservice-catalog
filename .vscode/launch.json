{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "auth-ms-basic-example",
      "program": "${workspaceFolder}/sandbox/auth-ms-basic-example/dist/index.js",
      "cwd": "${workspaceFolder}/sandbox/auth-ms-basic-example",
      "outFiles": [
        "${workspaceFolder}/sandbox/auth-ms-basic-example/dist/**/*.js"
      ],
      "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"],
      "protocol": "inspector",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "envFile": "${workspaceFolder}/sandbox/auth-ms-basic-example/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "auth-multitenant-example",
      "program": "${workspaceFolder}/sandbox/auth-multitenant-example/dist/index.js",
      "cwd": "${workspaceFolder}/sandbox/auth-multitenant-example",
      "outFiles": [
        "${workspaceFolder}/sandbox/auth-multitenant-example/dist/**/*.js"
      ],
      "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"],
      "protocol": "inspector",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "envFile": "${workspaceFolder}/sandbox/auth-multitenant-example/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "chat-notification-socketio-example/facade",
      "program": "${workspaceFolder}/sandbox/chat-notification-socketio-example/facade/dist/index.js",
      "cwd": "${workspaceFolder}/sandbox/chat-notification-socketio-example/facade",
      "outFiles": [
        "${workspaceFolder}/sandbox/chat-notification-socketio-example/facade/dist/**/*.js"
      ],
      "runtimeArgs": ["--preserve-symlinks", "--preserve-symlinks-main"],
      "protocol": "inspector",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "envFile": "${workspaceFolder}/sandbox/chat-notification-socketio-example/facade/.env"
    }
  ]
}
